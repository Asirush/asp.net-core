@using newAdmin.Modals
@model List<RoomProperty>

<table class="table table-hover">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (RoomProperty item in Model)
        {
            <tr>
                <td>@item.RoomPropertyId</td>
                <td>@item.Name</td>
                <td><a asp-action="Edit" asp-controller="RoomProperty" asp-route="@item.RoomPropertyId">Edit</a></td>
                <td><a asp-action="Delete" asp-controller="RoomProperty" asp-route="@item.RoomPropertyId">Delete</a></td>
            </tr>
        }
    </tbody>
</table>

<hr />
<a asp-action="Create" asp-controller="RoomProperty">Add</a>